$('.wizard').html('<%=j render "#{@card.current_step}_step" %>');
<% case @card.current_step %>
	<% when "general" %>
		selectize.cards();
		selectize.default();
	<% when "location" %>
		selectize.location();
		google_map.setGetCoord();
	<% when "team" %>
		autocomplete();
		hide_contact()
	<% when "extra" %>
		selectize.tags();
<% end %>
goToByScroll($('.head'));
