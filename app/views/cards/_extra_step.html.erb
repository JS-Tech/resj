<%= render layout: 'form' do |f| %>

	<%= f.label :website %>
	<%= f.text_field :website %><br>

	<%= f.label :password_digest %>
	<%= f.password_field :password_digest %><br>

	<%= f.label :affiliations %>
	<%= f.text_area :affiliations %>

	<%= f.label :tag_names %>
	<%= f.text_field :tag_names, class: "selectize-tags" %>
<% end %>