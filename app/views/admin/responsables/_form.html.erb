<%= form_for [:admin, @responsable] do |f| %>

	<%= render 'error_messages', object: f.object %>

	<table>
		<tr>
			<td><%= f.label :firstname %></td>
			<td><%= f.text_field :firstname %></td>
		</tr>
		<tr>
			<td><%= f.label :lastname %></td>
			<td><%= f.text_field :lastname %></td>
		</tr>
		<tr>
			<td><%= f.label :email %></td>
			<td><%= f.text_field :email %></td>
		</tr>
		<tr>
			<td><%= f.submit %></td>
		</tr>
	</table>

<% end %>