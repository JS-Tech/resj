<%= form_for @resource do |f| %>
	
	<%= render 'error_messages', object: f.object %>

	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :theme_id %>
	<%= f.collection_select :theme_ids, Theme.all, :id, :name, {include_blank: "Select themes"}, {class: "selectize-themes", multiple: true} %>

	<%= f.label :file %>
	<%= f.file_field :file %>
<% end %>