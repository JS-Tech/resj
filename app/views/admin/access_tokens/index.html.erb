<% if current_permission.allow_create?(params[:controller]) %>
	<%= form_for [:admin, AccessToken.new] do |f| %>
		<%= render 'error_messages', object: f.object %>

		<%= f.label :ownership_id%>
		<%= f.collection_select :ownership_id, Ownership.joins(:user).where(users: {firstname: "g_token"}), :id, :full_name, include_blank: "Please select" %>

		<%= f.label :exp_at %>
		<%= f.date_select :exp_at %>

		<%= f.submit %>
	<% end %>
<% end %>

<%= @table.present(buttons: "buttons") %>